<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<DialogPane prefHeight="175.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <ButtonType fx:id="fxILagi" buttonData="OK_DONE" text="Í lagi" />
    <ButtonType fx:id="fxHaettaVid" buttonData="CANCEL_CLOSE" text="Hætta við" />
    <content>
        <VBox alignment="CENTER" prefHeight="114.0" prefWidth="340.0">
            <children>
                <Label text="Erfiðleikastig">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <padding>
                        <Insets bottom="10.0" />
                    </padding>
                </Label>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Button fx:id="easyButton" mnemonicParsing="false" onAction="#easy" prefHeight="26.0" prefWidth="66.0" text="Létt" />
                        <Button fx:id="mediumButton" mnemonicParsing="false" onAction="#medium" prefHeight="26.0" prefWidth="66.0" text="Miðlungs" />
                        <Button fx:id="hardButton" mnemonicParsing="false" onAction="#hard" prefHeight="26.0" prefWidth="66.0" text="Erfitt" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin></HBox>
            </children>
            <padding>
                <Insets left="10.0" right="10.0" top="5.0" />
            </padding>
        </VBox>
    </content>
</DialogPane>
